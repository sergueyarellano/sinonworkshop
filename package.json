{
  "name": "sinonworkshop",
  "version": "1.0.0",
  "description": "Practical workshop with sinon.js and tape",
  "main": "lib/code.js",
  "scripts": {
    "lint": " eslint lib/* test/lib/* --color | snazzy",
    "precommit": "npm test",
    "coverage": "istanbul cover tape -- test/index.test.js",
    "tape": "blue-tape test/**/*.test.js | faucet",
    "test": "npm run lint && npm run coverage | tap-spec",
    "test-debug": "NODE_ENV=production browserify test/lib/marvel.test.js | browser-run -p 2222 | faucet",
    "watch:test": "nodemon --exec \"npm run test-debug\"",
    "watch:tape": "nodemon --exec \"npm run tape\""
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    }
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sergueyarellano/sinonworkshop.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sergueyarellano/sinonworkshop/issues"
  },
  "homepage": "https://github.com/sergueyarellano/sinonworkshop#readme",
  "dependencies": {
    "blue-tape": "^1.0.0",
    "browser-run": "^3.3.0",
    "browserify": "^13.1.1",
    "faucet": "0.0.1",
    "istanbul": "^0.4.5",
    "jquery": "^3.1.1",
    "jsdom": "^9.8.3",
    "jsdom-global": "^2.1.0",
    "nodemon": "^1.11.0",
    "request": "^2.79.0",
    "require-all": "^2.0.0",
    "sinon": "^1.17.6",
    "snazzy": "^5.0.0",
    "standard": "^8.5.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.2",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "eslint": "^3.10.2",
    "marvel-api": "^1.1.0",
    "uglify-js": "^2.7.4"
  }
}
