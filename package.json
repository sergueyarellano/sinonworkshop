{
  "name": "sinonworkshop",
  "version": "1.0.0",
  "description": "Practical workshop with sinon.js and QUnit",
  "main": "lib/code.js",
  "scripts": {
    "lint": " standard --verbose | snazzy",
    "precommit": "npm test",
    "coverage": "istanbul cover tape -- test/index.test.js",
    "tape": "tape test/**/*.test.js | faucet",
    "test": "npm run lint && npm run coverage | tap-spec",
    "watch:test": "nodemon --exec \"npm run tape\""
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sergueyarellano/sinonworkshop.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sergueyarellano/sinonworkshop/issues"
  },
  "homepage": "https://github.com/sergueyarellano/sinonworkshop#readme",
  "dependencies": {
    "blue-tape": "^1.0.0",
    "faucet": "0.0.1",
    "istanbul": "^0.4.5",
    "nodemon": "^1.11.0",
    "require-all": "^2.0.0",
    "sinon": "^1.17.6",
    "snazzy": "^5.0.0",
    "standard": "^8.5.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.2"
  }
}
